<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projectku">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".MateriDetailActivity"
            android:parentActivityName=".MateriActivity"></activity>
        <activity
            android:name=".AccountActivity"
            android:label="Account"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".SettingActivity"
            android:label="Setting"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Spash"
            android:label="@string/app_name"
            android:theme="@style/AppTheme2">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".RegisterActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".MainActivity"
            android:label="Home Screen"
            android:theme="@style/AppTheme.NoAction" />
        <activity
            android:name=".MapelActivity"
            android:label="Mata Pelajaran"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".MateriActivity"
            android:label="Materi"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".KelasActivity"
            android:label="Kelas"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".NilaiActivity"
            android:label="Nilai"
            android:theme="@style/AppTheme" />
    </application>

</manifest>